    <script type="text/javascript">
        $(function () {
            // Proxy created on the fly
            var agentStus = $.connection.agentStatusHubs;
            // Declare a function on the job hub so the server can invoke it
            agentStus.client.displayAgentStatus = function () {
                getData();
            };
            // Start the connection
            $.connection.hub.start();
            getData();
        });

        function getData() {
            var $tbl = $('#tblInfo');
            $.ajax({
                url: "/AgentStatus/Get",
                type: 'GET',
                datatype: 'json',
                success: function (data) {
                    $tbl.empty();
                    $.each(data.listAgStus, function (i, model) {
                        $tbl.append
                            (
                                '<tr>' +
                                '<td>' + model.Agent + '</td>' +
                                '<td>' + model.Region + '</td>' +
                                '<td>' + model.Available_Since + '</td>' +
                                '<td>' + model.Processing_Since + '</td>' +
                                '<td>' + model.Not_ready_since + '</td>' +
                                '<td>' + model.First_logged_in + '</td>' +
                                '<td>' + model.Last_logged_out + '</td>' +
                                '<td>' + model.Status + '</td>' +
                                '<tr>'
                            );
                    });
                }
            });
        }
    </script>
